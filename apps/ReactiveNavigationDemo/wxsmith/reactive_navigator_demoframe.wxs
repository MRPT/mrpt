									<object class="notebookpage">
										<object class="wxPanel" name="ID_PANEL2" variable="pnParamsGeneral" member="yes">
											<size>474,170</size>
											<object class="wxFlexGridSizer" variable="FlexGridSizer7" member="no">
												<cols>1</cols>
